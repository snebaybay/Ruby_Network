<h4> Users you may want to connect with: </h4>

	<table>
		<tr>
			<th> Name </th>
			<th> Action </th>
		</tr>

		<tr>

		<% @users.each do |user| %>
		<% if @current_user.id != user.id%>
		<% @friendship.each do |friend| %>
		<% if @friend.friend_id != user.id%>
		<td> <a href="/users/<%= user.id%>"> <%= user.name%> </a> </td>
		
		<td> <a href="/friendships/create/<%= user.id%>">Connect</a></td>
		</tr>
		<%end%>
		<%end%>
		<%end%>
		<%end%>
	</table>

	<a href="/professional_profile"> My Profile </a>

